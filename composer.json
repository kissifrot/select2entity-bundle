{
    "name": "kissifrot/select2entity-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that integrates Select2 as a drop-in replacement for a standard entity field on a Symfony form.",
    "keywords": ["symfony","select2", "typeahead","autocomplete"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Keatinge",
            "email": "tetranz@gmail.com"
        },
        {
            "name": "Philippe Villiers",
            "email": "kissifrot+gh@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/orm": ">=2.4",
        "twig/twig": "^3.0.4",
        "symfony/property-access": ">=7.0",
        "symfony/dependency-injection": ">=7.0",
        "symfony/http-kernel": ">=7.0",
        "symfony/form": ">=7.0",
        "symfony/config": ">=7.0",
        "symfony/routing": ">=7.0"
    },
    "autoload": {
        "psr-4": { "Tetranz\\Select2EntityBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11"
    }
}
